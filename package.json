{
  "name": "nanomatch",
  "description": "Fast, minimal glob matcher for node.js. ",
  "version": "0.1.0",
  "homepage": "https://github.com/jonschlinkert/nanomatch",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/nanomatch",
  "bugs": {
    "url": "https://github.com/jonschlinkert/nanomatch/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "lib"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "arr-diff": "^3.0.0",
    "arr-union": "^3.1.0",
    "array-unique": "^0.3.2",
    "debug": "^2.2.0",
    "define-property": "^0.2.5",
    "extend-shallow": "^2.0.1",
    "fragment-cache": "^0.1.0",
    "is-glob": "^3.0.0",
    "isobject": "^2.1.0",
    "normalize-path": "^2.0.1",
    "object.pick": "^1.1.2",
    "snapdragon": "^0.7.0",
    "to-regex": "^0.1.1"
  },
  "devDependencies": {
    "async-each": "^1.0.1",
    "bash-glob": "^0.1.1",
    "cross-spawn": "^4.0.2",
    "delete": "^0.3.2",
    "fs-exists-sync": "^0.1.0",
    "gulp-format-md": "^0.1.11",
    "is-absolute": "^0.2.5",
    "micromatch": "^2.3.11",
    "minimatch": "^3.0.3",
    "mkdirp": "^0.5.1",
    "mocha": "^3.0.2",
    "multimatch": "^2.1.0",
    "write": "^0.3.2",
    "yargs-parser": "^4.0.0"
