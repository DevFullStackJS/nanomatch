  "verb": {
    "toc": true,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "expand-brackets",
        "extglob",
        "micromatch"
      ]
    },
    "reflinks": [
      "micromatch",
      "node-glob",
      "verb",
      "verb-generate-readme"
    ],
    "lint": {
      "reflinks": true
    }
